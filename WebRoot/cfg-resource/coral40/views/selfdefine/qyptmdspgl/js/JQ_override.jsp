/**
 *  二次开发：复写自定义列表
 */
function _override_grid (ui) {
	if(ui.options.number === 1){
		ui.beforeInitGrid = function(setting) {
			setting.multiselect = false;//单选
			setting.singleselect = true;//添加单选框
			return setting;
		}

		ui.addCallback("addSpToMd",function(o){
			if(null == o) return;
			var rowData = o.rowData;
			if(null == rowData) return;
			var mdid = ui.getSelectedRowId();
			var spData = $.loadJson($.contextPath + '/qyptmdspgl!checkSpid.json?spid=' + JSON.stringify(rowData) +'&mdid=' +mdid);
			var exsitSpmc = new Array();
			if(spData!=""&&spData!=null){
				for(var i = 0;i<spData.length;i++){
					exsitSpmc.push(spData[i].SPMC);
				}
				CFG_message("商品"+JSON.stringify(exsitSpmc)+"已存在！", "warning");
			}
            var detailGrid = ui.getDetailGrid();
            detailGrid.reload();
		});
	}
	// ui.assembleData 就是 configInfo
	//console.log("override grid!");
	//ui.getAction = function () {
	//	return $.contextPath + "/appmanage/show-module";
	//};
};
