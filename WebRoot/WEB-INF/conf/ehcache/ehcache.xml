           diskSpoolBufferSizeMB="20"
           timeToIdleSeconds="0"
           timeToLiveSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
        <persistence strategy="localTempSwap"/>
    </cache>
    <cache name="APP_GRID"
           maxEntriesLocalHeap="10000"
           maxEntriesLocalDisk="10000000"
           eternal="false"
           diskSpoolBufferSizeMB="20"
           timeToIdleSeconds="0"
           timeToLiveSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
        <persistence strategy="localTempSwap"/>
    </cache>
    <cache name="APP_COLUMN"
           maxEntriesLocalHeap="10000"
           maxEntriesLocalDisk="10000000"
           eternal="false"
           diskSpoolBufferSizeMB="20"
           timeToIdleSeconds="0"
           timeToLiveSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
        <persistence strategy="localTempSwap"/>
    </cache>
    <cache name="APP_SORT"
           maxEntriesLocalHeap="10000"
           maxEntriesLocalDisk="10000000"
